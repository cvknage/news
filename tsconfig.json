{
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es5",
        "sourceMap": true,
        "inlineSourceMap": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "outDir": "target/precompiled",
        "module": "commonJS"
    },
    "exclude": [
        "node_modules"
    ],
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    }
}
